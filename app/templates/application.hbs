  {{#paper-sidenav-container class="site-nav-container"}}
    {{#paper-sidenav
      class="md-whiteframe-z2 site-sidenav"
      lockedOpen="gt-sm"
      name="app-main"
      open=isSideNavOpen
      onToggle=(action (mut isSideNavOpen))}}
      {{#paper-toolbar class="site-content-toolbar" }}
        <div class="md-toolbar-tools">
          <img src="/img/aelogo.png" height="50">&nbsp;&nbsp;<strong>IPFMS</strong>
        </div>
      {{/paper-toolbar}}
      {{#paper-content}}
        {{#paper-list}}
          {{#paper-item action="transitionTo"  href="/" param="home"}}
            {{paper-icon icon="home" size="lg"}}
            Home
          {{/paper-item}}
          {{#paper-item action="transitionTo"  href="/config" param="config"}}
            {{paper-icon icon="build" size="lg"}}
            Configuration
          {{/paper-item}}
          {{#paper-item action="transitionTo"  href="/print" param="print"}}
            {{paper-icon icon="print" size="lg"}}
            Print
          {{/paper-item}}
        {{/paper-list}}
      {{/paper-content}}
    {{/paper-sidenav}}

    <div id="main" class="flex layout-column" tabindex="-1" role="main">
      {{outlet}}
    </div>
  {{/paper-sidenav-container}}

  {{paper-toaster parent="#main"}}

